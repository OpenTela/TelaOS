<app>
  <ui default="/main">
    <page id="main">
      <label align="center" y="2%" font="32" color="#0ff">
        Brainfuck Demo
      </label>

      <input id="stdin" x="5%" y="10%" w="65%" h="40"
             bind="_stdin" placeholder="Please enter your name..." bgcolor="#222"/>

      <button x="73%" y="10%" w="22%" h="40" bgcolor="#2196F3"
              onclick="start">Run</button>

      <label x="5%" y="22%" w="90%" h="68%"
             color="#0f0" bgcolor="#111" radius="8"
             font="16" text-align="left top">
        {_stdout}
      </label>
    </page>
  </ui>

  <state>
    <string name="_stdout" default=""/>
    <string name="_stdin" default=""/>
  </state>

